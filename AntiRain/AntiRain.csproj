<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net5.0</TargetFramework>
        <Authors>Yukari316</Authors>
        <Copyright>Yukari316</Copyright>
        <Platforms>AnyCPU;x64;x86</Platforms>
        <Version>0.4.2</Version>
        <FileVersion>0.4.2.0</FileVersion>
        <Nullable>warnings</Nullable>
    </PropertyGroup>

    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
        <DocumentationFile></DocumentationFile>
    </PropertyGroup>

    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x86'">
        <DocumentationFile />
    </PropertyGroup>

    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
        <DocumentationFile />
    </PropertyGroup>

    <ItemGroup>
        <EmbeddedResource Include="Config\Res\InitGlobalConfig.yaml" />
        <EmbeddedResource Include="Config\Res\InitUserConfig.yaml" />
        <EmbeddedResource Update="Resource\Font.resx">
          <Generator>ResXFileCodeGenerator</Generator>
          <LastGenOutput>Font.Designer.cs</LastGenOutput>
        </EmbeddedResource>
        <EmbeddedResource Update="Resource\Fonts.resx">
          <Generator>ResXFileCodeGenerator</Generator>
          <LastGenOutput>Fonts.Designer.cs</LastGenOutput>
        </EmbeddedResource>
        <None Remove="Resource\JetBrainsMono-Regular.ttf" />
        <EmbeddedResource Include="Resource\JetBrainsMono-Regular.ttf" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="BeetleX.FastHttpApi" Version="1.9.5" />
        <PackageReference Include="BilibiliApi" Version="0.3.10" />
        <PackageReference Include="JetBrains.Annotations" Version="2021.3.0" />
        <PackageReference Include="PyLibSharp.Requests" Version="1.2.10" />
        <PackageReference Include="SharpYaml" Version="1.8.0" />
        <PackageReference Include="Sora" Version="1.0.0-rc.24" />
        <PackageReference Include="sqlSugarCore" Version="5.0.4.2" />
        <PackageReference Include="System.Drawing.Common" Version="5.0.2" />
        <PackageReference Include="System.Text.Encoding.CodePages" Version="5.0.0" />
    </ItemGroup>

    <ItemGroup>
        <Compile Update="Config\Res\InitRes.Designer.cs">
            <DesignTime>True</DesignTime>
            <AutoGen>True</AutoGen>
            <DependentUpon>InitRes.resx</DependentUpon>
        </Compile>
        <Compile Update="Resource\Font.Designer.cs">
          <DesignTime>True</DesignTime>
          <AutoGen>True</AutoGen>
          <DependentUpon>Font.resx</DependentUpon>
        </Compile>
        <Compile Update="Resource\Fonts.Designer.cs">
          <DesignTime>True</DesignTime>
          <AutoGen>True</AutoGen>
          <DependentUpon>Fonts.resx</DependentUpon>
        </Compile>
    </ItemGroup>

    <ItemGroup>
        <EmbeddedResource Update="Config\Res\InitRes.resx">
            <Generator>ResXFileCodeGenerator</Generator>
            <LastGenOutput>InitRes.Designer.cs</LastGenOutput>
        </EmbeddedResource>
    </ItemGroup>

</Project>
